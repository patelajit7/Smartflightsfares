@using Common
@{
    int aff = Utility.GetAffiliateId(Request);
}

<div style="height:129px"></div>
<div id="fit">
    <div class="modal-content modal-window-style">
        <div class="modal-body">
            <div class="phone-modal-content col-12 pdlr-0 d-flex">
                <div class="operator-col col-3 d-flex pdlr-0">
                    <div class="call-girl-section pdlr-0">
                        <div class="call-girl">

                        </div>
                        <div class="available-section">
                            <div class="available-circle"></div>
                            <div class="available-text">Available Now</div>
                        </div>
                    </div>
                </div>

                <div class="checklist-col col-9  pdlr-0 d-flex flex-wrap">
                    <div class="number-section shockwave col-md-6 col-sm-11 col-xs-12 pdlr-0">
                        <div class="head-text"> For phone only flight deals,<br/>upto 30% cheaper </div>
                        <div class="international">

                            @if (Common.Utility.PortalSettings.ResponseTagAffiliates.Contains(aff))
                            {
                                <a href="tel:@ViewBag.TollFreeNumber" id="@aff" class="call-mCenter">
                                    <div class="tel-container us phone_click_event" onclick="rTapClickToCall(359597)">
                                        <div class="tel-box">
                                            <p>Call now</p>
                                            <span class="rTapNumber359597" onclick="rTapClickToCall(359597)">@ViewBag.TollFreeNumber</span>
                                        </div>
                                        <div class="call-img shake">
