@model IEnumerable<Infrastructure.HelpingModel.API.Contract>
@using Infrastructure.HelpingModel.API;
@if (Model != null && Model.Count() > 0)
{
    Contract minContract = Model.FirstOrDefault();
    {
        Html.RenderPartial("~/views/flights/partial/_ListingInstancePopup.cshtml", minContract);
    }
    int count = 0;
    foreach (Contract item in Model)
    {
        {
            Html.RenderPartial("~/views/flights/partial/_Contract.cshtml", item);

            count++;
        }
        if (HttpContext.Current.Session["webmode"] != null)
        {
            Html.RenderPartial("~/views/flights/partial/_mode.cshtml", item);
        }
    }
}
else
{
    <div class="listing" id="containerListing">
        <div id="flights-list" class="flight-data">
            <section class="clearAllFilter">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#664c00" class="bi bi-exclamation-triangle-fill" viewBox="0 0 16 16">
                    <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
                </svg>
                No results match your selections. <a onclick="contractFilter.resetFilter();" class="undertxt"> Clear your filters to see exact search results</a>.
            </section>
        </div>
    </div>
}